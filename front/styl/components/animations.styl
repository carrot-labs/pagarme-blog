@keyframes bounceUpIn
  0%
    transform translateY(100%)
  60%
    transform translateY(0%)
  80%
    transform translateY(1%)
  100%
    transform translateY(0%)

@keyframes bounceUpOut
  0%
    transform translateY(0%)
    opacity 1
  80%
    transform translateY(100%)
  100%
    transform translateY(100%)
    opacity 0
